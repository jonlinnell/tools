{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": "warn",
    "semi": [2, "never"],
    "comma-dangle": ["error", "always-multiline"]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react",
    "jest"
  ],
  "env": {
    "browser": true,
    "jest/globals": true,
  }
}